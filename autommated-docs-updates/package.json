{
  "name": "autommated-docs-updates",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev:doc-automation": "ts-node src/index.ts doc-automation /tmp/repos/docs /tmp/repos/foundation-ui",
    "dev:doc-automation:mock": "cross-env USE_MOCK_SERVICES=true ts-node src/index.ts doc-automation /tmp/repos/docs /tmp/repos/foundation-ui",
    "dev:doc-automation:real": "cross-env USE_MOCK_SERVICES=false ts-node src/index.ts doc-automation /tmp/repos/docs /tmp/repos/foundation-ui",
    "dev:checks": "ts-node src/index.ts checks /tmp/repos/docs /tmp/repos/foundation-ui",
    "dev:checks:mock": "cross-env USE_MOCK_SERVICES=true ts-node src/index.ts checks /tmp/repos/docs /tmp/repos/foundation-ui",
    "dev:checks:real": "cross-env USE_MOCK_SERVICES=false ts-node src/index.ts checks /tmp/repos/docs /tmp/repos/foundation-ui",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/node": "^24.1.0",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "dotenv": "^17.2.0",
    "@langchain/anthropic": "^0.2.0",
    "@langchain/core": "^0.2.0",
    "langchain": "^0.2.0",
    "@octokit/rest": "^20.0.0"
  }
}
