{
  "name": "genesis-docs-mcp-server",
  "version": "0.0.1",
  "description": "genesis-docs-mcp-server MCP server",
  "type": "module",
  "bin": {
    "genesis-docs-mcp-server": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run copy-docs && npm run build:src",
    "build:src": "tsc && mcp-build",
    "debug": "npx @modelcontextprotocol/inspector dist/index.js",
    "copy-docs": "copyfiles -u 1 ../docs/**/*.md ../docs/**/*.mdx dist",
    "start": "node dist/index.js",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "copyfiles": "^2.4.1",
    "fuse.js": "^7.1.0",
    "globby": "^14.1.0",
    "mcp-framework": "^0.2.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.19.0"
  }
}
